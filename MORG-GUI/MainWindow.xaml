<Window x:Class="MORG_GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MORG_GUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="550" Width="440">
    <Window.Background>
        <SolidColorBrush Color="AliceBlue"/>
    </Window.Background>
    <Canvas x:Name="myCanvas" Background="White" HorizontalAlignment="Center" Height="400" Margin="0,10,0,0" VerticalAlignment="Top" Width="400">
            <Border BorderBrush="Black" BorderThickness="2" Height="400" Width="400"/>
        <TextBox x:Name="textBox" Height="75" Canvas.Left="0" TextWrapping="Wrap" Text="TextBox" Canvas.Top="420" Width="400"/>
    </Canvas>
</Window>
